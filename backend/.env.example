# Database Configuration
DATABASE_URL=postgresql://user:password@host:5432/database?sslmode=disable
DATABASEURL=postgresql://user:password@host:5432/database?sslmode=disable

# Redis Configuration
REDIS_URL=redis://user:password@host:6379/0
REDISURL=redis://user:password@host:6379/0
CACHE_REDIS_URL=redis://user:password@host:6379/0

# Application
NODE_ENV=production
PORT=9000
WORKER_MODE=shared

# Security (generate your own)
JWT_SECRET=your-jwt-secret-here
COOKIE_SECRET=your-cookie-secret-here

# CORS
STORE_CORS=http://your-store-domain:8000
ADMIN_CORS=http://your-admin-domain:7001
AUTH_CORS=http://your-auth-domain:7001

# Admin
DISABLE_ADMIN=false
MEDUSA_ADMIN_ONBOARDING_TYPE=default

# PostgreSQL SSL Disable
PGSSLMODE=disable
NODE_TLS_REJECT_UNAUTHORIZED=0